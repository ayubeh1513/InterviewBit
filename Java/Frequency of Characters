public class Solution {
    public ArrayList<Integer> solve(String A) {
        ArrayList<Integer> a = new ArrayList<>();
        for(int i=0; i<26; i++){
            a.add(0);
        }
        for(char i : A.toCharArray()){
            int index = i - 'a';
            a.set(index, a.get(index) + 1);
        }
        
        return a;
    }
}
