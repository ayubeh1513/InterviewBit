public class Solution {
    public int solve(ArrayList<Integer> A, ArrayList<Integer> B, int C) {
        int cg = 0;
        int cl = 0;
        for(int i=0; i<A.size(); i++){
            if(A.get(i)>C){
                cg++;
            }
        }
        
        for(int i=0; i<B.size(); i++){
            if(B.get(i)<C){
                cl++;
            }
        }
        
        if(cg>cl){
            return cg;
        } else {
            return cl;
        }
    }
}
