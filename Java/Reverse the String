public class Solution {
    public String solve(String A) {
        A.trim();
        String[] ar = A.split("\\s+");
        String[] arr = new String[ar.length];
        int k=0;
        for(int i=ar.length-1; i>=0; i--){
            arr[k]=ar[i];
            k++;
        }
        String a = String.join(" ", arr);
        return a.trim();
    }
}
